                  <Insets bottom="15.0" left="15.0" top="15.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="newRangeEndCoordinateTextField" layoutX="793.0" layoutY="25.0" prefHeight="27.0" prefWidth="87.0" promptText="To (i.e C3)">
